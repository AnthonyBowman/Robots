    ttk::style configure Labelframe -font "$vTcl(actual_gui_font_dft_desc)"
    labelframe "$top.lab75" \
        -font "TkDefaultFont" -foreground $vTcl(actual_gui_fg) \
        -text "Auto Control" -background $vTcl(actual_gui_bg) -height 115 \
        -width 179 
    vTcl:DefineAlias "$top.lab75" "LabelframeAutoControl" vTcl:WidgetProc "Toplevel1" 1
### SPOT dump_widget_opt A
    set site_3_0 $top.lab75
    ttk::style configure TButton -background $vTcl(actual_gui_bg)
    ttk::style configure TButton -foreground $vTcl(actual_gui_fg)
    ttk::style configure TButton -font "$vTcl(actual_gui_font_dft_desc)"
    ttk::style configure TButton -background $vTcl(actual_gui_bg)
    ttk::style configure TButton -foreground $vTcl(actual_gui_fg)
    ttk::style configure TButton -font "$vTcl(actual_gui_font_dft_desc)"
    ttk::button "$site_3_0.tBu76" \
        -takefocus {} -text "Obstacle Avoidance" -compound left 
    vTcl:DefineAlias "$site_3_0.tBu76" "TBtnObstacleAvoidance" vTcl:WidgetProc "Toplevel1" 1
### SPOT dump_widget_opt A
    ttk::style configure TButton -background $vTcl(actual_gui_bg)
    ttk::style configure TButton -foreground $vTcl(actual_gui_fg)
    ttk::style configure TButton -font "$vTcl(actual_gui_font_dft_desc)"
    ttk::style configure TButton -background $vTcl(actual_gui_bg)
    ttk::style configure TButton -foreground $vTcl(actual_gui_fg)
    ttk::style configure TButton -font "$vTcl(actual_gui_font_dft_desc)"
    ttk::button "$site_3_0.tBu77" \
        -takefocus {} -text "Buetooth Follow" -compound left 
    vTcl:DefineAlias "$site_3_0.tBu77" "TBtnBluetoothFollow" vTcl:WidgetProc "Toplevel1" 1
### SPOT dump_widget_opt A
    ttk::style configure TButton -background $vTcl(actual_gui_bg)
    ttk::style configure TButton -foreground $vTcl(actual_gui_fg)
    ttk::style configure TButton -font "$vTcl(actual_gui_font_dft_desc)"
    ttk::style configure TButton -background $vTcl(actual_gui_bg)
    ttk::style configure TButton -foreground $vTcl(actual_gui_fg)
    ttk::style configure TButton -font "$vTcl(actual_gui_font_dft_desc)"
    ttk::button "$site_3_0.tBu78" \
        -takefocus {} -text "TBA" -compound left 
    vTcl:DefineAlias "$site_3_0.tBu78" "TBtnTBA" vTcl:WidgetProc "Toplevel1" 1
### SPOT dump_widget_opt A
    place $site_3_0.tBu76 \
        -in $site_3_0 -x 0 -relx 0.063 -y 0 -rely 0.174 -width 142 \
        -relwidth 0 -height 28 -relheight 0 -anchor nw -bordermode ignore 
    place $site_3_0.tBu77 \
        -in $site_3_0 -x 0 -relx 0.063 -y 0 -rely 0.435 -width 142 \
        -relwidth 0 -height 28 -relheight 0 -anchor nw -bordermode ignore 
    place $site_3_0.tBu78 \
        -in $site_3_0 -x 0 -relx 0.063 -y 0 -rely 0.696 -width 142 \
        -relwidth 0 -height 28 -relheight 0 -anchor nw -bordermode ignore 
    ###################
    # SETTING GEOMETRY
    ###################
    place $top.lab47 \
        -in $top -x 0 -relx 0.72 -y 0 -rely 0.057 -width 0 -relwidth 0.26 \
        -height 0 -relheight 0.821 -anchor nw -bordermode ignore 
    place $top.lab53 \
        -in $top -x 0 -relx 0.557 -y 0 -rely 0.667 -width 0 -relwidth 0.158 \
        -height 0 -relheight 0.219 -anchor nw -bordermode ignore 
    place $top.lab54 \
        -in $top -x 0 -relx 0.186 -y 0 -rely 0.667 -width 0 -relwidth 0.159 \
        -height 0 -relheight 0.219 -anchor nw -bordermode ignore 
    place $top.lab64 \
        -in $top -x 0 -relx 0.186 -y 0 -rely 0.057 -width 0 -relwidth 0.532 \
        -height 0 -relheight 0.6 -anchor nw -bordermode ignore 
    place $top.lab66 \
        -in $top -x 0 -relx 0.012 -y 0 -rely 0.057 -width 0 -relwidth 0.173 \
        -height 0 -relheight 0.829 -anchor nw -bordermode ignore 
    place $top.lab75 \
        -in $top -x 0 -relx 0.348 -y 0 -rely 0.667 -width 0 -relwidth 0.208 \
        -height 0 -relheight 0.219 -anchor nw -bordermode ignore 

    vTcl:FireEvent $base <<Ready>>
}

proc 36 {args} {return 1}


Window show .
set btop1 ""
if {$vTcl(borrow)} {
    set btop1 .bor[expr int([expr rand() * 100])]
    while {[lsearch $btop1 $vTcl(tops)] != -1} {
        set btop1 .bor[expr int([expr rand() * 100])]
    }
}
set vTcl(btop) $btop1
Window show .top1 $btop1
if {$vTcl(borrow)} {
    $btop1 configure -background plum
}

